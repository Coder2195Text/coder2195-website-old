{
    "$schema": "http://json-schema.org/schema",
    "required": [
        "homeTitle",
        "homeHeading",
        "flip"
    ],
    "properties": {
        "flip": {
            "type": "boolean",
            "description": "flip the text?"
        },
        "homeHeading": {
            "type": "string",
            "description": "heading on home page"
        }, 
        "homeTitle": {
            "type": "string",
            "description": "title on home page that appears on the tab"
        }
    }
}